{
  "name": "Nest Stream Add-on",
  "url": "https://github.com/TUO_USERNAME/hassio-nest-stream",
  "maintainer": "Tuo Nome <tuo@email.com>",
  "addons": [
    {
      "name": "Nest Stream",
      "slug": "nest_stream",
      "version": "1.0.0",
      "stage": "stable",
      "description": "Streams Nest Doorbell RTSP to Home Assistant",
      "url": "https://github.com/TUO_USERNAME/hassio-nest-stream/tree/main/nest_stream",
      "arch": ["armhf", "armv7", "aarch64", "i386", "amd64"],
      "startup": "system",
      "boot": "auto",
      "host_network": true,
      "init": false,
      "hassio_api": true,
      "homeassistant_api": true,
      "options": {
        "client_id": "YOUR_CLIENT_ID",
        "client_secret": "YOUR_CLIENT_SECRET",
        "refresh_token": "YOUR_REFRESH_TOKEN",
        "project_id": "YOUR_PROJECT_ID",
        "device_id": "YOUR_DEVICE_ID",
        "stream_port": 8081
      },
      "schema": {
        "client_id": "str",
        "client_secret": "str",
        "refresh_token": "str",
        "project_id": "str",
        "device_id": "str",
        "stream_port": "int"
      }
    }
  ]
}
